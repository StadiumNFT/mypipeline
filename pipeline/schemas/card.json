{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CardRecord",
  "type": "object",
  "required": ["sku", "cat", "conf"],
  "properties": {
    "sku": { "type": "string" },
    "cat": { "type": "string", "enum": ["sports", "marvel", "pokemon", "other"] },
    "brand": { "type": "string" },
    "set": { "type": "string" },
    "year": { "type": "integer" },
    "player": { "type": "string" },
    "character": { "type": "string" },
    "num": { "type": "string" },
    "subset": { "type": "string" },
    "variant": { "type": "string" },
    "serial": { "type": "string" },
    "auto": { "type": "boolean" },
    "mem": { "type": "boolean" },
    "grade": { "type": "string" },
    "cond": { "type": "string", "enum": ["NM", "EX", "VG", "raw-estimate"] },
    "notes": { "type": "string" },
    "price_est": { "type": "number" },
    "conf": { "type": "number", "minimum": 0, "maximum": 1 }
  },
  "additionalProperties": true
}
